package fr.ensma.a3.ia.mvc.textfield.automate;

public interface IEtatText {
	public void enableText() throws TransitionTextException;
	public void disableText() throws TransitionTextException;
	public void valEntree() throws TransitionTextException;
	public void backspace() throws TransitionTextException;
	public void vide() throws TransitionTextException;
	public void newVal() throws TransitionTextException;
	public void nonVide() throws TransitionTextException;
	
}
