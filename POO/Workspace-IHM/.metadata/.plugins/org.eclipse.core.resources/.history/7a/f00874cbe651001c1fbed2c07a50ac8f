package fr.ensma.a3.ia.mvc.textfield;

import java.util.List;

public class textMdl {
	
	private textCtrl control ;
	private textVue vue ;
	
	private List<ITextObserver> mesAbos;
	
	public textMdl() {
		control = new textCtrl(this) ;
		vue = control.getVue() ;
		mesAbos = new ArrayList<ITextObserver>() ;
	}
	

}
